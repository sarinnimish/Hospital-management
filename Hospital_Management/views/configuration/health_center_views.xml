<?xml version="1.0" encoding="UTF-8"?>
<odoo>
       <!-- Form View -->
    <record id="view_health_center_form" model="ir.ui.view">
        <field name="name">health.center.form</field>
        <field name="model">health.center</field>
        <field name="arch" type="xml">
            <form string="health center">
                <group>
                    <group>
                        <field name="name"/>
                        <field name="phone"/>
                         <field name="email"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

        <!-- list view -->
    <record id="view_health_center_list" model="ir.ui.view">
        <field name="name">health.center.list</field>
        <field name="model">health.center</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="phone"/>
                 <field name="email"/>
            </list>
        </field>
    </record>

        <!-- action view -->
     <record id="action_health_center" model="ir.actions.act_window">
        <field name="name">health.center</field>
        <field name="res_model">health.center</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>