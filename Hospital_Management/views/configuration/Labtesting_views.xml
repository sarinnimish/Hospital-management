<?xml version="1.0" encoding="UTF-8"?>
<odoo>
       <!-- Form View -->
    <record id="view_lab_testing_form" model="ir.ui.view">
        <field name="name">lab.testing.form</field>
        <field name="model">lab.testing</field>
        <field name="arch" type="xml">
            <form string="Lab Testing Units">
                <group>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

        <!-- list view -->
    <record id="view_lab_testing_list" model="ir.ui.view">
        <field name="name">lab.testing.list</field>
        <field name="model">lab.testing</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
            </list>
        </field>
    </record>

        <!-- action view -->
     <record id="action_lab_testing" model="ir.actions.act_window">
        <field name="name">Lab Testing Units</field>
        <field name="res_model">lab.testing</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>