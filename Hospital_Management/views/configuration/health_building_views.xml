<?xml version="1.0" encoding="UTF-8"?>
<odoo>
       <!-- Form View -->
    <record id="view_health_center_building_form" model="ir.ui.view">
        <field name="name">health.center.building.form</field>
        <field name="model">health.center.building</field>
        <field name="arch" type="xml">
            <form string="health center building">
                <group>
                    <group>
                        <field name="build_name"/>
                        <field name="build_institute"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

        <!-- list view -->
    <record id="view_health_center_building_list" model="ir.ui.view">
        <field name="name">health.center.building.list</field>
        <field name="model">health.center.building</field>
        <field name="arch" type="xml">
            <list>
                <field name="build_name"/>
                <field name="build_institute"/>
            </list>
        </field>
    </record>

        <!-- action view -->
     <record id="action_health_center_building" model="ir.actions.act_window">
        <field name="name">health.center.building</field>
        <field name="res_model">health.center.building</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>