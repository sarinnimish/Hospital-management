<?xml version="1.0" encoding="UTF-8"?>
<odoo>
       <!-- Form View -->
    <record id="view_lab_test_type_form" model="ir.ui.view">
        <field name="name">lab.test.type.form</field>
        <field name="model">lab.test.type</field>
        <field name="arch" type="xml">
            <form string="Lab Test Types">
                <group>
                    <group>
                        <field name="name"/>
                        <field name="labtest"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

        <!-- list view -->
    <record id="view_lab_test_type_list" model="ir.ui.view">
        <field name="name">lab.test.type.list</field>
        <field name="model">lab.test.type</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="labtest"/>
            </list>
        </field>
    </record>

        <!-- action view -->
     <record id="action_lab_test_type" model="ir.actions.act_window">
        <field name="name">Lab Test Types</field>
        <field name="res_model">lab.test.type</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>