<odoo>
<record id="view_upload_excel_form" model="ir.ui.view">
    <field name="name">button.upload.form</field>
    <field name="model">button.upload</field>
    <field name="arch" type="xml">
        <form string="Upload Excel">
            <group>
                <field name="file" filename="filename"/>
                <field name="filename"/>
            </group>
            <footer>
                <button name="upload_excel_file" string="Upload" type="object" class="btn-primary"/>
                <button string="Cancel" class="btn-secondary" special="cancel"/>
            </footer>
        </form>
    </field>
</record>
</odoo>
